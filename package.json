{
  "name": "svn-ij-history",
  "displayName": "SVN IntelliJ-style History",
  "description": "SVN history viewer inspired by IntelliJ with advanced search, timeline grouping, and quick diff.",
  "version": "1.2.1",
  "publisher": "SimoneBestazza",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/Besta/svn-ij-history.git"
  },
  "homepage": "https://github.com/Besta/svn-ij-history#readme",
  "bugs": {
    "url": "https://github.com/Besta/svn-ij-history/issues"
  },
  "engines": {
    "vscode": "^1.96.0"
  },
  "categories": [
    "SCM Providers",
    "Visualization"
  ],
  "keywords": [
    "svn",
    "subversion",
    "history",
    "intellij",
    "diff"
  ],
  "activationEvents": [],
  "main": "./dist/extension.js",
  "contributes": {
    "viewsContainers": {
      "panel": [
        {
          "id": "svn-ij-history-container",
          "title": "SVN History",
          "icon": "$(history)"
        }
      ]
    },
    "views": {
      "svn-ij-history-container": [
        {
          "id": "svn-ij-history.history-tree",
          "name": "History",
          "type": "tree",
          "icon": "$(history)"
        },
        {
          "type": "tree",
          "id": "svn-ij-history.details-tree",
          "name": "Details",
          "icon": "$(info)",
          "when": "svn-ij-history:hasCommitSelected"
        }
      ]
    },
    "commands": [
      {
        "command": "svn-ij-history.clearDetails",
        "title": "Clear Details",
        "category": "SVN History",
        "icon": "$(clear-all)"
      },
      {
        "command": "svn-ij-history.clearFilter",
        "title": "Clear Filter",
        "category": "SVN History",
        "icon": "$(close)"
      },
      {
        "command": "svn-ij-history.search",
        "title": "Search",
        "category": "SVN History",
        "icon": "$(search)"
      },
      {
        "command": "svn-ij-history.filterUser",
        "title": "Filter by User",
        "category": "SVN History",
        "icon": "$(account)"
      },
      {
        "command": "svn-ij-history.refresh",
        "title": "Refresh",
        "category": "SVN History",
        "icon": "$(refresh)"
      },
      {
        "command": "svn-ij-history.showFileHistory",
        "title": "Show History",
        "category": "SVN History",
        "icon": "$(history)"
      },
      {
        "command": "svn-ij-history.toggleAnnotate",
        "title": "Toggle Annotate",
        "category": "SVN History",
        "icon": "$(info)"
      },
      {
        "command": "svn-ij-history.showAnnotate",
        "title": "Show Annotations",
        "category": "SVN History",
        "icon": "$(info)"
      },
      {
        "command": "svn-ij-history.hideAnnotate",
        "title": "Hide Annotations",
        "category": "SVN History",
        "icon": "$(info)"
      },
      {
        "command": "svn-ij-history.openDiff",
        "title": "Compare with Previous",
        "category": "SVN History",
        "icon": "$(diff)"
      },
      {
        "command": "svn-ij-history.openLocal",
        "title": "Open File",
        "category": "SVN History",
        "icon": "$(go-to-file)"
      },
      {
        "command": "svn-ij-history.revertFile",
        "title": "Get this Version",
        "category": "SVN History",
        "icon": "$(desktop-download)"
      },
      {
        "command": "svn-ij-history.copyRevision",
        "title": "Copy Revision Number",
        "category": "SVN History"
      },
      {
        "command": "svn-ij-history.copyRelPath",
        "title": "Copy Path Relative",
        "category": "SVN History"
      },
      {
        "command": "svn-ij-history.copyAbsPath",
        "title": "Copy Path Absolute",
        "category": "SVN History"
      },
      {
        "command": "svn-ij-history.copyFileName",
        "title": "Copy File Name",
        "category": "SVN History"
      }
    ],
    "menus": {
      "view/title": [
        {
          "command": "svn-ij-history.clearDetails",
          "when": "view == svn-ij-history.details-tree",
          "group": "navigation"
        },
        {
          "command": "svn-ij-history.clearFilter",
          "when": "view == svn-ij-history.history-tree && svn-ij-history:isFiltered",
          "group": "navigation@0"
        },
        {
          "command": "svn-ij-history.search",
          "when": "view == svn-ij-history.history-tree",
          "group": "navigation@1"
        },
        {
          "command": "svn-ij-history.filterUser",
          "when": "view == svn-ij-history.history-tree",
          "group": "navigation@2"
        },
        {
          "command": "svn-ij-history.refresh",
          "when": "view == svn-ij-history.history-tree",
          "group": "navigation@3"
        }
      ],
      "explorer/context": [
        {
          "command": "svn-ij-history.showFileHistory",
          "when": "resourceScheme == file",
          "group": "7_modification"
        }
      ],
      "editor/context": [
        {
          "command": "svn-ij-history.showFileHistory",
          "when": "resourceScheme == file",
          "group": "7_modification"
        },
        {
          "command": "svn-ij-history.showAnnotate",
          "when": "resourceScheme == file && !svn-ij-history:isAnnotateActive",
          "group": "7_modification"
        },
        {
          "command": "svn-ij-history.hideAnnotate",
          "when": "resourceScheme == file && svn-ij-history:isAnnotateActive",
          "group": "7_modification"
        }
      ],
      "view/item/context": [
        {
          "command": "svn-ij-history.copyRevision",
          "when": "view == svn-ij-history.history-tree && viewItem == commit-item",
          "group": "1_clipboard"
        },
        {
          "command": "svn-ij-history.copyRevision",
          "when": "view == svn-ij-history.details-tree && viewItem == commit-header",
          "group": "1_clipboard"
        },
        {
          "command": "svn-ij-history.openDiff",
          "when": "view == svn-ij-history.details-tree && viewItem == file-item",
          "group": "inline@1"
        },
        {
          "command": "svn-ij-history.openDiff",
          "when": "view == svn-ij-history.details-tree && viewItem == file-item",
          "group": "2_modification"
        },
        {
          "command": "svn-ij-history.openLocal",
          "when": "view == svn-ij-history.details-tree && viewItem == file-item",
          "group": "inline@2"
        },
        {
          "command": "svn-ij-history.openLocal",
          "when": "view == svn-ij-history.details-tree && viewItem == file-item",
          "group": "2_modification"
        },
        {
          "command": "svn-ij-history.revertFile",
          "when": "view == svn-ij-history.details-tree && viewItem == file-item",
          "group": "inline@3"
        },
        {
          "command": "svn-ij-history.revertFile",
          "when": "view == svn-ij-history.details-tree && viewItem == file-item",
          "group": "2_modification"
        },
        {
          "command": "svn-ij-history.copyRelPath",
          "when": "view == svn-ij-history.details-tree && viewItem == file-item",
          "group": "3_clipboard"
        },
        {
          "command": "svn-ij-history.copyAbsPath",
          "when": "view == svn-ij-history.details-tree && viewItem == file-item",
          "group": "3_clipboard"
        },
        {
          "command": "svn-ij-history.copyFileName",
          "when": "view == svn-ij-history.details-tree && viewItem == file-item",
          "group": "3_clipboard"
        }
      ],
      "editor/title": [
        {
          "command": "svn-ij-history.showFileHistory",
          "when": "resourceScheme == file",
          "group": "navigation"
        },
        {
          "command": "svn-ij-history.showAnnotate",
          "when": "resourceScheme == file && !svn-ij-history:isAnnotateActive",
          "group": "navigation"
        },
        {
          "command": "svn-ij-history.hideAnnotate",
          "when": "resourceScheme == file && svn-ij-history:isAnnotateActive",
          "group": "navigation"
        }
      ],
      "editor/title/context": [
        {
          "command": "svn-ij-history.showFileHistory",
          "when": "resourceScheme == file",
          "group": "7_modification"
        },
        {
          "command": "svn-ij-history.showAnnotate",
          "when": "resourceScheme == file && !svn-ij-history:isAnnotateActive",
          "group": "7_modification"
        },
        {
          "command": "svn-ij-history.hideAnnotate",
          "when": "resourceScheme == file && svn-ij-history:isAnnotateActive",
          "group": "7_modification"
        }
      ],
      "editor/lineNumber/context": [
        {
          "command": "svn-ij-history.showAnnotate",
          "when": "resourceScheme == file && !svn-ij-history:isAnnotateActive",
          "group": "7_modification"
        },
        {
          "command": "svn-ij-history.hideAnnotate",
          "when": "resourceScheme == file && svn-ij-history:isAnnotateActive",
          "group": "7_modification"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run package",
    "compile": "webpack",
    "watch": "webpack --watch",
    "package": "webpack --mode production --devtool hidden-source-map",
    "compile-tests": "tsc -p . --outDir out",
    "watch-tests": "tsc -p . -w --outDir out",
    "pretest": "npm run compile-tests && npm run compile && npm run lint",
    "lint": "eslint src",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/mocha": "^10.0.10",
    "@types/node": "22.x",
    "@types/vscode": "^1.96.0",
    "@vscode/codicons": "^0.0.44",
    "@vscode/test-cli": "^0.0.12",
    "@vscode/test-electron": "^2.5.2",
    "eslint": "^9.39.2",
    "ts-loader": "^9.5.4",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.54.0",
    "webpack": "^5.104.1",
    "webpack-cli": "^6.0.1"
  },
  "dependencies": {
    "@vscode/webview-ui-toolkit": "^1.4.0",
    "fast-xml-parser": "^5.4.1"
  },
  "icon": "icon.png"
}